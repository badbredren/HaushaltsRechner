<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="de.swproject.hbapp"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="10"
        android:targetSdkVersion="15" />

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/appName"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".StartTabActivity"
            android:label="@string/titleStartActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <activity  android:name=".NewEntryActivity" />        
        <activity android:name=".EntryListActivity" /> 
        <activity android:name=".UpdateEntryActivity" />
        
    </application>

</manifest>